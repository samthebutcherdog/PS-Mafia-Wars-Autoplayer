{
   "background_page": "background.html",
   "content_scripts": [ {
      "js": [ "mwplayer.user.js" ],
      "exclude_globs": [ "http://mwfb.zynga.com/mwfb/*#*",
                         "http://facebook.mafiawars.com/mwfb/*#*" ],
      "matches": [ "http://facebook.mafiawars.com/mwfb/remote/html_server.php*",
                   "http://mwfb.zynga.com/mwfb/remote/html_server.php*",
                   "http://apps.facebook.com/inthemafia/*",
                   "http://apps.new.facebook.com/inthemafia/*",
                   "http://www.facebook.com/connect/prompt_feed*" ],
      "all_frames":true
   } ],
   "description": "PS Facebook Mafia Wars Autoplayer (MWAP)",
   "icons": {
      "128": "mwap128.png",
      "48": "mwap48.png"
   },
   "permissions": [ "http://code.google.com/p/mwplayer/*" ],
   "name": "Autoplayer for Mafia Wars (Facebook)",
   "version": "1.1.586",
   "minimum_chrome_version": "4.0"
}
